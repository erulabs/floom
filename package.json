{
  "name": "opsjs",
  "version": "0.0.1",
  "description": "The streaming infrastructure build system",
  "main": "lib/ops.js",
  "scripts": {
    "test": "env NODE_ENV=test node node_modules/mocha/bin/_mocha --reporter spec",
    "debug": "env NODE_ENV=debug node node_modules/mocha/bin/_mocha --reporter spec",
    "coverage": "env NODE_ENV=test istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "todos": "grep -n '@todo' lib/*.js || : && grep -n '@todo' bin/*.js || :"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:erulabs/opsjs.git"
  },
  "keywords": [
    "streaming",
    "infrastructure",
    "devops",
    "automation"
  ],
  "author": "Seandon 'erulabs' Mooy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/erulabs/opsjs/issues"
  },
  "homepage": "https://github.com/erulabs/opsjs",
  "devDependencies": {
    "coveralls": "^2.11.2",
    "gulp": "^3.8.11",
    "gulp-exec": "^2.1.1",
    "gulp-jshint": "^1.10.0",
    "gulp-watch": "^4.2.4",
    "istanbul": "^0.3.13",
    "jshint-stylish": "^1.0.1",
    "mocha": "^2.2.4",
    "run-sequence": "^1.0.2",
    "should": "^6.0.1"
  },
  "dependencies": {
    "highland": "^2.4.0",
    "orchestrator": "^0.3.7",
    "ssh2": "^0.4.6",
    "ssh2shell": "^1.4.1",
    "winston": "^1.0.0"
  }
}
